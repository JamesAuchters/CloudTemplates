{
	"$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
        "DeploymentLocation": {
            "type": "string",
            "DefaultValue": "australiaeast",
            "allowedValues": [
                "australiasoutheast",
                "australiaeast"
            ],
            "metadata": {
                "decription": "The Default Deployment Location (AU South East/AU East)"
            }
        }
    },
	"variables": {},
	"functions": [],
	"resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "location": "australiaeast",
            "name": "ResourceGroups",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri":"https://raw.githubusercontent.com/JamesAuchters/CloudTemplates/master/Palo-Alto/ResourceGroup.json",
                    "contentVersion":"1.0.0.0"
                },
                "parameters": {}
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "location": "[parameters('DeploymentLocation')]",
            "resourceGroup": "[reference('ResourceGroups').outputs.cloudshellRgName.value]",
            "name": "CloudShell",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri":"https://raw.githubusercontent.com/JamesAuchters/CloudTemplates/master/Palo-Alto/CloudShell.json",
                    "contentVersion":"1.0.0.0"
                },
                "parameters": {}
            }
        }
    ],
	"outputs": {}
}